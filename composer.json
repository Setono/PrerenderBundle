{
    "name": "setono/prerender-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle that integrates 'prerender' services, i.e. rendertron or prerender.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/http-client": "^4.4 || ^5.0",
        "symfony/http-client-contracts": "^2.4",
        "symfony/http-foundation": "^4.4 || ^5.0.7",
        "symfony/http-kernel": "^4.4 || ^5.1.5"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.0",
        "psalm/plugin-symfony": "^3.0",
        "roave/security-advisories": "dev-latest",
        "setono/code-quality-pack": "^2.1.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\PrerenderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\PrerenderBundle\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpspec": "phpspec run",
        "phpunit": "phpunit"
    }
}
