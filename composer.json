{
    "name": "setono/prerender-bundle",
    "description": "A Symfony bundle that integrates 'prerender' services, i.e. rendertron or prerender.io",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": ">=7.4",
        "league/uri": "^6.5",
        "league/uri-components": "^2.4",
        "setono/bot-detection-bundle": "^1.9",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/event-dispatcher": "^5.4 || ^6.0",
        "symfony/http-client": "^5.4 || ^6.0",
        "symfony/http-client-contracts": "^2.5 || ^3.0",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.6",
        "psalm/plugin-phpunit": "^0.18 || ^0.19",
        "psalm/plugin-symfony": "^5.0",
        "setono/code-quality-pack": "^2.4",
        "symfony/process": "^5.4 || ^6.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Setono\\PrerenderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\PrerenderBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpunit": "phpunit"
    }
}
